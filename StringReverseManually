public class Main
{
	public static void main(String[] args) {
		String s1="Hello";
		int n=s1.length();
		char []arr=new char[n];
        for(int i=0;i<s1.length();i++){
            char ch=s1.charAt(i);
            	arr[i]=ch;
        }
        for(int i=s1.length()-1;i>=0;i--){
            System.out.print(arr[i]);
        }
	}
}

