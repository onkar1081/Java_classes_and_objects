import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		System.out.println("Enter number");
		int num=s.nextInt();
		int c=0;
		for(int i=2;i<num;i++){
		    if(num%i==0){
		        c=1;
		    }
	}
	if(c==1){
	    System.out.println("Not Prime number");
	}if(c==0){
	    System.out.println("Prime number");
	}
}
}
